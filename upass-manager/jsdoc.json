{
  "source": {
    "include": ["src", "pages", "backend-api", "backend-common"],
    "includePattern": "\\.(js|jsx|ts|tsx)$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown", "node_modules/jsdoc-plugin-typescript"],
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/docdash",
    "verbose": true
  },
  "typescript": {
    "moduleRoot": "./",
    "configFile": "./tsconfig.jsdoc.json"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "typescript"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "docdash": {
    "static": true,
    "sort": true,
    "search": true,
    "collapse": true,
    "typedefs": true,
    "removeQuotes": "none",
    "scripts": [],
    "menu": {
      "GitHub": {
        "href": "https://github.com/yourusername/upass-manager",
        "target": "_blank"
      }
    }
  }
}