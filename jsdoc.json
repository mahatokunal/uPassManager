{
  "source": {
    "include": [
      "upass-manager/src",
      "upass-manager/backend-api",
      "upass-manager/pages/api"
    ],
    "exclude": [
      "node_modules",
      ".next"
    ],
    "includePattern": ".+\\.js(x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/component"
  ],
  "tags": {
    "allowUnknownTags": ["component"]
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/better-docs"
  },
  "templates": {
    "better-docs": {
      "name": "UPass Manager Documentation"
    }
  }
}